<view id="van-tabbar-placeholder" class="van-tabbar-placeholder">
    <view></view>
</view>
<van-tabbar active="{{active}}" bind:change="handleTabChange">
    <van-tabbar-item>
        <image slot="icon" src="{{ home.normal }}" class="image" />
        <image slot="icon-active" src="{{ home.active }}" class="image" />
        主页
    </van-tabbar-item>
    <block wx:if="{{!!approvalTabEnable}}">
        <van-tabbar-item>
            <image slot="icon" src="{{ approval.normal }}" class="image" />
            <image slot="icon-active" src="{{ approval.active }}" class="image" />
            审批
        </van-tabbar-item>
    </block>
    <van-tabbar-item>
        <image slot="icon" src="{{ mine.normal }}" class="image" />
        <image slot="icon-active" src="{{ mine.active }}" class="image" />
        我的
    </van-tabbar-item>
</van-tabbar>