.traveler-list {
  padding: 0 20rpx; }
  .traveler-list .traveler-item {
    padding: 24rpx 32rpx;
    background: white;
    margin-top: 24rpx;
    display: flex;
    border-radius: 4px; }
    .traveler-list .traveler-item .traveler-info {
      flex: 1; }
      .traveler-list .traveler-item .traveler-info .employee-name {
        font-size: 32rpx;
        font-weight: bold;
        color: #333333;
        margin-bottom: 24rpx; }
      .traveler-list .traveler-item .traveler-info .doc-info {
        font-size: 24rpx;
        color: #666666; }
        .traveler-list .traveler-item .traveler-info .doc-info .doc-item {
          display: flex;
          margin-bottom: 12rpx;
          line-height: 1; }
          .traveler-list .traveler-item .traveler-info .doc-info .doc-item .label {
            width: 220rpx; }
        .traveler-list .traveler-item .traveler-info .doc-info .doc-item:last-child {
          margin-bottom: 0; }
    .traveler-list .traveler-item .operate {
      font-size: 40rpx;
      color: #cbcbcb;
      margin-top: 32rpx; }
  .traveler-list .delete-btn {
    color: #fb0101;
    text-align: center;
    font-size: 24rpx;
    width: 196rpx;
    position: relative;
    top: 50%;
    transform: translateY(-50%); }
    .traveler-list .delete-btn iconfont {
      font-size: 36rpx; }
